package ExtraMapAssignments;

class Studnt
{
	int id;
	String name;
	Studnt()
	{}
	Studnt(int id, String nm)
	{
		this.id=id;
		name=nm;
	}
	@Override
	public String toString() 
	{
		return "Student id=" + id + ", Student name=" + name;
	}
	
}
