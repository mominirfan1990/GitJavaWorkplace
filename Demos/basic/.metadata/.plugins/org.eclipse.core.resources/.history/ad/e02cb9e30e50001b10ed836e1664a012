//19. WAP to create a Map which stores another Map as a value.

package HashMapAssignments;

import java.util.HashMap;
import java.util.Map;

public class Assign_19
{
	Map<Integer, String> map;
	Map<String, Map> map1;
	
	public void creatMAp() 
	{
		map = new HashMap<>();
		map.put(23, "Honda");
		map.put(62, "Hero");
		map.put(3, "TVS");
		map.put(814, "Honda");
		
		map1=new HashMap<>();
		map1.put("Bike", map);
		
		System.out.println(map1);
	}
	Map<Integer, String> map2;
	Map<String, Map> map3;
	public void creatvoter()
	{
		map2=new HashMap<>();
		map2.put(2334, "Irfan");
		map2.put(2335, "Vishal");
		map2.put(2336, "omkar");
		map2.put(2337, "Deepak");
		
		map3=new HashMap<>();
		map3.put("Vote 2", map2);
	}
	public static void main(String[] args)
	{
		Assign_19 as= new Assign_19();
		as.creatMAp();
	}

}
